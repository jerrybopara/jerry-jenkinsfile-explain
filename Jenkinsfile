pipeline {
    agent any
    
    stages {
        stage("Build") {
            
            steps {
                echo 'building an application...'                   
            } 
        } 

        stage("Test") {
            
            steps {
                echo 'testing the application...'
            }
        }

        stage("Deploy") {

            steps {
                echo 'Deploying the application...'
            }
        }


    } // end of stages
} // end of pipeline

